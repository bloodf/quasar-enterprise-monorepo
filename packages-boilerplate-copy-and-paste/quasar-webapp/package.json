{
  "name": "@quasar-enterprise-monorepo/quasar-webapp",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "author": "Heitor Ramon Ribeiro <heitor.ramon@gmail.com>",
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "build": "quasar build",
    "build:ci": "quasar build",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:fix": "npm run lint:fix:src && npm run lint:fix:test",
    "lint:fix:src": "eslint --fix \"src/**/*.{js,vue,ts,tsx}\"",
    "lint:fix:test": "eslint --fix \"test/**/*.{js,vue,ts,tsx}\"",
    "lint:src": "eslint --ext .js,.vue,.ts,.tsx src",
    "lint:test": "eslint --ext .js,.vue,.ts,.tsx test",
    "precommit": "lint-staged",
    "serve": "quasar dev",
    "serve:test:coverage": "quasar serve tests/jest/coverage/lcov-report/ --port 8788",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "test": "jest --updateSnapshot --passWithNoTests",
    "test:coverage": "jest  --coverage --passWithNoTests",
    "test:ui": "majestic",
    "test:watch": "jest  --watch --passWithNoTests",
    "test:watchAll": "jest  --watchAll --passWithNoTests",
    "update:check": "ncu",
    "update:update": "ncu -u"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "dependencies": {
    "@quasar-enterprise-monorepo/base-store": "*",
    "@quasar-enterprise-monorepo/text-utils": "*",
    "@quasar-enterprise-monorepo/vue-router-utils": "*",
    "@quasar-enterprise-monorepo/vuex-utils": "*",
    "@quasar/app": "^2.1.14",
    "@quasar/extras": "^1.9.13",
    "@vue/composition-api": "^0.6.7",
    "core-js": "^3.8.2",
    "fast-memoize": "^2.5.2",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "quasar": "^1.15.0",
    "transform-object-keys": "^1.0.3",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-i18n": "^8.22.3",
    "vue-loadable": "^0.2.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.9",
    "vuelidate": "^0.7.6",
    "vuelidate-property-decorators": "^1.0.28",
    "vuex": "^3.6.0",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@quasar-enterprise-monorepo/quasar-testing": "*",
    "@quasar-enterprise-monorepo/eslint-config-monorepo": "*",
    "@quasar-enterprise-monorepo/eslint-config-monorepo-import": "*",
    "@quasar-enterprise-monorepo/eslint-config-monorepo-typescript": "*",
    "@quasar-enterprise-monorepo/eslint-config-monorepo-vue": "*",
    "@quasar-enterprise-monorepo/eslint-config-monorepo-vue-i18n": "*",
    "jest": "^26.6.3",
    "majestic": "^1.8.0",
    "dotenv-webpack": "^2.0.0",
    "@types/vuelidate": "^0.7.13",
    "@quasar/cli": "^1.1.3",
    "@quasar/quasar-app-extension-dotenv": "^1.0.5",
    "@quasar/quasar-app-extension-testing": "^1.0.3",
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.1.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@vue/test-utils": "^1.1.2",
    "hygen": "^6.0.4",
    "vue-styleguidist": "^4.34.2",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "engines": {
    "node": ">= 12",
    "yarn": ">= 1"
  },
  "capacitorId": "",
  "cordovaId": "org.cordova.quasar.app",
  "productName": "Quasar App",
  "shortProductName": "Web App"
}
