{
  "name": "@quasar-enterprise-monorepo/quasar-app-extension-ui-component",
  "version": "0.0.1",
  "author": "Heitor Ramon Ribeiro <heitor.ramon@gmail.com>",
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "build": "vue-cli-service build --target lib --name new_vue_ts_component",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "lint:fix:src": "vue-cli-service lint --fix component/ && vue-cli-service lint --fix src/",
    "lint:fix:test": "vue-cli-service lint --fix tests/",
    "precommit": "lint-staged",
    "serve": "vue-cli-service serve",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build",
    "test": "vue-cli-service test:unit --passWithNoTests",
    "test:coverage": "vue-cli-service test:unit --coverage --passWithNoTests",
    "test:ui": "majestic",
    "test:unit": "vue-cli-service test:unit --updateSnapshot --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "types:vue": "vuedts component && vuedts src",
    "update:check": "ncu",
    "update:update": "ncu -u"
  },
  "files": [
    "src",
    "dist"
  ],
  "main": "src/index.ts",
  "dependencies": {
    "@quasar/extras": "^1.9.13",
    "core-js": "^3.8.2",
    "quasar": "^1.15.0",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@quasar-enterprise-monorepo/quasar-testing": "*",
    "@quasar-enterprise-monorepo/eslint-config-monorepo": "*",
    "@quasar-enterprise-monorepo/eslint-config-monorepo-import": "*",
    "@quasar-enterprise-monorepo/eslint-config-monorepo-typescript": "*",
    "@quasar-enterprise-monorepo/eslint-config-monorepo-vue": "*",
    "@quasar-enterprise-monorepo/eslint-config-monorepo-vue-i18n": "*",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-eslint": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-plugin-unit-jest": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "1.1.2",
    "babel-loader": "^8.2.2",
    "hygen": "^6.0.4",
    "majestic": "^1.8.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "~4.1.3",
    "vue-cli-plugin-quasar": "^3.0.1",
    "vue-cli-plugin-styleguidist": "^4.34.2",
    "vue-template-compiler": "^2.6.12",
    "wallaby-vue-compiler": "^1.0.6"
  },
  "engines": {
    "node": ">= 12",
    "yarn": ">= 1"
  }
}
