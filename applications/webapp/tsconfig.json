{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@graphql/*": ["src/graphql/*"],
      "app/*": ["./*"],
      "assets/*": ["src/assets/*"],
      "boot/*": ["src/boot/*"],
      "components/*": ["src/components/*"],
      "config/*": ["src/config/*"],
      "cypress/*": ["test/cypress/*"],
      "drivers/*": ["src/drivers/*"],
      "enums/*": ["src/enums/*"],
      "hooks/*": ["src/hooks/*"],
      "jest/*": ["test/jest/*"],
      "jest/tests/*": ["test/jest/__tests__/*"],
      "jest/utils/*": ["test/jest/utils/*"],
      "layouts/*": ["src/layouts/*"],
      "loaders/*": ["test/loaders/*"],
      "mixins/*": ["src/mixins/*"],
      "pages/*": ["src/pages/*"],
      "services/*": ["src/services/*"],
      "src/*": ["src/*"],
      "store/*": ["src/store/*"],
      "utils/*": ["src/utils/*"]
    },
    "baseUrl": "."
  },
  "exclude": [
    "_templates",
    "dist",
    "node_modules",
    ".quasar"
  ]
}
