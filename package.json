{
  "name": "quasar-enterprise-monorepo",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "workspaces": {
    "packages": [
      "applications/*",
      "clients/*",
      "packages/*",
      "utils/*",
      "vue-plugins/*",
      "vuex-common-stores/*"
    ]
  },
  "scripts": {
    "postinstall": "yarn manypkg fix",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "build": "lerna run build:ci --stream",
    "clean": "manypkg exec npx rimraf node_modules && manypkg exec npx rimraf coverage && manypkg exec npx rimraf dist && rm -rf node_modules && npx rimraf yarn.lock",
    "commit": "git-cz",
    "exec:in": "lerna run --stream --scope ",
    "install:packages": "yarn && yarn bootstrap",
    "lint": "lerna run lint --stream",
    "lint:fix": "lerna run lint:fix --stream",
    "lint:fix:src": "lerna run lint:fix:src --stream",
    "new-version": "lerna version --conventional-commits --message 'chore: publish'",
    "test": "lerna run test --stream",
    "test:coverage": "lerna run test:coverage --stream",
    "test:ui": "majestic",
    "test:silent": "jest --reporters jest-silent-reporter --collectCoverage=false",
    "test:jest": "jest --collectCoverage=false",
    "update:check": "ncu && lerna run update:check --stream",
    "update:update": "ncu -u && lerna run update:update --stream && yarn install --force && yarn manypkg fix"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.13.0",
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@commitlint/config-lerna-scopes": "^12.0.0",
    "@manypkg/cli": "^0.17.0",
    "@types/chart.js": "^2.9.30",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.20",
    "@types/jsdom": "^16.2.6",
    "@types/node": "^14.14.31",
    "@types/vfile-message": "^2.0.0",
    "@types/webpack": "^4.41.26",
    "commitizen": "^4.2.3",
    "concurrently": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-loader": "^4.0.2",
    "husky": "^4.3.8",
    "hygen": "^6.0.4",
    "jest": "^26.6.3",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-localstorage-mock": "^2.4.6",
    "jest-silent-reporter": "^0.4.0",
    "jest-watch-lerna-packages": "^1.1.0",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^0.6.1",
    "jest-watch-yarn-workspaces": "^1.1.0",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.4",
    "majestic": "^1.8.0",
    "npm-check-updates": "^11.1.8",
    "serverless": "2.25.2",
    "serverless-prune-plugin": "1.4.3",
    "serverless-s3-sync": "1.16.1",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5",
    "webpack": "^4.44.2",
    "webpack-manifest-plugin": "^3.0.0"
  },
  "engines": {
    "node": ">= 12",
    "yarn": ">= 1"
  }
}
